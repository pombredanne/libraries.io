<%= link_to 'Delete', admin_project_suggestion_path(project_suggestion), method: :delete, class: 'btn btn-danger pull-right'  %>
<h3>
  <%= link_to project_suggestion.project, project_path(project_suggestion.project.to_param) %>
  <% if project_suggestion.user %>
    <small>
      By <%= link_to project_suggestion.user, mail_to(project_suggestion.user.email) %>
      <%= timeago_tag project_suggestion.created_at %>
    </small>
  <% end %>
</h3>
<% if project_suggestion.valid_repository_change? %>
  <p>
    <strong>repository_url</strong>: <%= project_suggestion.repository_url %>
  </p>
<% end %>
<% if project_suggestion.valid_license_change? %>
  <p>
    <strong>license</strong>: <%= project_suggestion.licenses %>
  </p>
<% end %>
<% if project_suggestion.valid_status_change?  %>
  <p>
    <strong>status</strong>: <%= project_suggestion.status %>
  </p>
<% end %>
<p>
  <strong>notes</strong>: <%= project_suggestion.notes %>
</p>
<hr>
